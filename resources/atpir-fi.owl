<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY provo "http://www.w3.org/ns/prov#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY xml "http://www.w3.org/XML/1998/namespace" >
    <!ENTITY skos "http://www.w3.org/TR/skos-reference#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY lkif-core "http://www.estrellaproject.org/lkif-core/lkif-core.owl#" >
]>


<rdf:RDF xmlns="http://purl.oclc.org/NET/atpir-fi#"
     xml:base="http://purl.oclc.org/NET/atpir-fi"
     xmlns:lkif-core="http://www.estrellaproject.org/lkif-core/lkif-core.owl#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:provo="http://www.w3.org/ns/prov#"
     xmlns:skos="http://www.w3.org/TR/skos-reference#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://purl.org/NET/atpir-fi">
        <rdfs:label>Flight Incident Ontology</rdfs:label>
        <rdfs:comment>This is an ontology on Flight Incidents.
The ontology expresses the main flight disruptions that frame the air transport dispute market, such as a) baggage incidents (delayed, damaged and missing baggage); b) flight incidents (delayed, cancelled, denied flights); and c) service incidents (unfair commercial practises, bad quality service and irresponsiveness)</rdfs:comment>
        <dct:contributor>Víctor Rodríguez-Doncel</dct:contributor>
        <dct:contributor>Cristiana Santos</dct:contributor>
        <owl:versionInfo>1.0</owl:versionInfo>
        <dct:license rdf:resource="http://purl.org/NET/rdflicense/cc-by"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/NET/atpir-fi#ActualTimeOfArrival -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ActualTimeOfArrival">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ActualTimeOfDeparture -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ActualTimeOfDeparture">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#DomesticFlight -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#DomesticFlight">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#FlightDate -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#FlightDate">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#FlightNumber -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#FlightNumber">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#IntraCommunityFlight -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#IntraCommunityFlight">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#LongHaulFlight -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#LongHaulFlight">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#MediumHaulFlight -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#MediumHaulFlight">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfArrival -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfArrival">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfDeparture -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfDeparture">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ShortHaulFlight -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ShortHaulFlight">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#SingularFlight -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#SingularFlight">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#Flight"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/TR/skos-reference#broader -->

    <owl:AnnotationProperty rdf:about="&skos;broader"/>
    


    <!-- http://www.w3.org/TR/skos-reference#closeMatch -->

    <owl:AnnotationProperty rdf:about="&skos;closeMatch"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/NET/atpir-fi#causedBy -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#causedBy">
        <rdfs:comment>declares the incident that has caused a complaint</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#Incident"/>
        <skos:closeMatch rdf:resource="&provo;wasGeneratedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/atpir-fi#hasDepartureAirport -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/atpir-fi#hasDepartureAirport">
        <rdfs:comment>declares the departure airport for a flight
</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#Airport"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/atpir-fi#hasDestinationAirport -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/atpir-fi#hasDestinationAirport">
        <rdfs:comment>declares the destination airport for a flight
</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#Airport"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/atpir-fi#hasFlightDetails -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/atpir-fi#hasFlightDetails">
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#ConnectingFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#DirectFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#DomesticFlight"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#IntraCommunityFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#LongHaulFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#MediumHaulFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#NonStopFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#RoundTripFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#ShortHaulFlight"/>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#SingularFlight"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/atpir-fi#underContextOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/atpir-fi#underContextOf">
        <rdfs:comment>declares the context that is related to an incident</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/NET/atpir-fi#Context"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Incident"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <rdf:Description rdf:about="&owl;topObjectProperty">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#FlightIncident"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/NET/atpir-fi#ActualTimeOfArrival -->

    <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ActualTimeOfArrival">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ActualTimeOfDeparture -->

    <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ActualTimeOfDeparture">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#FlightDate -->

    <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#FlightDate">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#FlightNumber -->

    <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#FlightNumber">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfArrival -->

    <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfArrival">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfDeparture -->

    <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/atpir-fi#ScheduledTimeOfDeparture">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/atpir-fi#hasLocation -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/atpir-fi#hasLocation">
        <rdfs:comment>Declares the location of an Airport</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/NET/atpir-fi#Airport"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/atpir-fi#reasonForCancellation -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/atpir-fi#reasonForCancellation">
        <rdfs:comment>Cause originating a flight cancellation.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/atpir-fi#CancelledFlight"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/NET/atpir-fi#BaggageIncident -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/atpir-fi#BaggageIncident">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Incident"/>
        <rdfs:comment>Incident related to a baggage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#CancelledFlight -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/atpir-fi#CancelledFlight">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#FlightIncident"/>
        <rdfs:comment>A cancelled flight means the non-operation of a flight that was previously planned, Article 5 of the EC Regulation 261/2004.
</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#DelayedFlight -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/atpir-fi#DelayedFlight">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#FlightIncident"/>
        <rdfs:comment>Delayed flight refers to a flight whose departure time is later than the departure time originally scheduled, Article 6 of the EU Regulation
</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#DeniedFlight -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/atpir-fi#DeniedFlight">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#FlightIncident"/>
        <rdfs:comment>Denied flight means the air carrier refusal to carry passengers on a flight, although they have presented themselves for boarding under the conditions laid down in Article 3(2) of the (EC) Regulation, except where there are reasonable grounds to deny them boarding, such as reasons of health, safety or security, or inadequate travel documentation.
</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#FlightIncident -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/atpir-fi#FlightIncident">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#FlightIncident"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Incident"/>
        <rdfs:comment>Incident related to a flight.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/atpir-fi#ServiceIncident -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/atpir-fi#ServiceIncident">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Incident"/>
        <rdfs:comment>Incident related to the service provided.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#Airport -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#Airport">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Context"/>
        <rdfs:comment>An airport is a location where aircraft such as fixed-wing aircraft, helicopters, and blimps take off and land. Aircraft may be stored or maintained at an airport. An airport consists of at least one surface such as a runway for a plane to take off and land, a helipad, or water for takeoffs and landings, and often includes buildings such as control towers, hangars and terminal buildings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#BadQuality -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#BadQuality">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#ServiceIncident"/>
        <rdfs:comment>Incident related to a bad quality service provided by the air carrier.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#BankrupcyIncident -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#BankrupcyIncident">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Incident"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#ConnectingFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#ConnectingFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>A connecting flight is a flight with an intermediate stop and a change of aircraft (possibly a change of airlines and flight number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#Context -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#Context">
        <rdfs:comment>A set of circumstances of facts that surround a flight.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#DamagedBaggage -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#DamagedBaggage">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#BaggageIncident"/>
        <rdfs:comment>Incident related to damaged baggage</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#DelayedBaggage -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#DelayedBaggage">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#BaggageIncident"/>
        <rdfs:comment>Incident related to a delayed baggage</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#DirectFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#DirectFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>A direct flight  is a flight with one or more intermediate stops but no change of aircraft and no change of flight numbers. The stop over may either be to get new passengers (or allow some to disembark) or a mere technical stop over (i.e., for refuelling).</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#DomesticFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#DomesticFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>A domestic flight is a form of commercial flight where the departure and the arrival take place in the same country.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#Downgrading -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#Downgrading">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#SeatMisplacement"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#Event -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#Event"/>
    


    <!-- http://purl.org/NET/atpir-fi#Flight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#Flight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Context"/>
        <rdfs:comment>A flight is a scheduled route by plane between designated airports. </rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#Incident -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#Incident">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Event"/>
        <rdfs:comment>An ocurrence or event related to the flight, baggage or service.</rdfs:comment>
        <skos:broader rdf:resource="&lkif-core;Occurrence"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#IntraCommunityFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#IntraCommunityFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>Intra-Community flight means the movement of an air carrier between two Community airports, without any stopovers, and which does not start from or end at a non-Community airport.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#Irresponsiveness -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#Irresponsiveness">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#ServiceIncident"/>
        <rdfs:comment>Incident related to the difficulties in getting information from the air carrier on where and how to complain and on claim redress (e.g. no phone number, no email or all telephone lines busy, no response to the complaint). </rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#LongHaulFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#LongHaulFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>Long-haul flight is a flight whose length typically requires over six and a half hours to cover and is often a non-stop flight.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#LostBaggage -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#LostBaggage">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/atpir-fi#BaggageIncident"/>
        <rdfs:comment>Incident related to lost baggage</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#MediumHaulFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#MediumHaulFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>A medium-haul flight is often defined as having a flight length that takes one and a half hours or more to complete, which roughly correlates to an absolute distance over 800 km.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#NonStopFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#NonStopFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>Non-stop flight is a flight made without intermediate stops between source and destination.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#RoundTripFlight -->

    <owl:Class rdf:about="http://purl.org/NET/atpir-fi#RoundTripFlight">
        <rdfs:subClassOf rdf:resource="http://purl.org/NET/atpir-fi#Flight"/>
        <rdfs:comment>Round trip flight combines a one-way trip and the return trip back to the airport of origin.
</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/atpir-fi#SeatMisplacement -->

